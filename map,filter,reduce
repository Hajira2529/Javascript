let array=[5,4,6,7,8];

console.log(array.map(element => {
    let square = element * element
    return square;
}
))

console.log(array.map(element => element * element
))

console.log(array.filter(element => {
    if(element%2 == 0){
        return element
    }
}))

console.log(array.reduce((accumlation,element) => {
    return accumlation + element
}))